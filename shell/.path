#!/usr/bin/env bash

# custom bins
export PATH="$HOME/.bin:$PATH"

# golang
export GOPATH="$HOME/repos/go"
export GOBIN="$HOME/repos/go/bin"
export PATH="$GOPATH/bin:$PATH"

# rust
export PATH="$HOME/.cargo/bin:$PATH"

# node.js
export PATH="/usr/local/bin:$PATH"

# gnu flavored utils
export PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"

# Python 2.7
export PATH="/Users/segersand/Library/Python/2.7/bin:$PATH"

# ruby gems
export PATH="/Users/simon/.gem/ruby/2.6.0/bin:$PATH"

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/segersand/google-cloud-sdk/path.zsh.inc' ]; then source '/Users/segersand/google-cloud-sdk/path.zsh.inc'; fi

export PATH="$HOME/bin:$PATH"

# Export for virtualenvwrapper
export PATH="/Users/segersand/Library/Python/2.7/bin:$PATH"
